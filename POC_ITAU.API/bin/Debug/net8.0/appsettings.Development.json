{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "Kafka_URI": "localhost:29092"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Datadog.Logs" ],
    "WriteTo": [
      {
        "Name": "DatadogLogs",
        "Args": {
          "ApiKey": "SUA_API_KEY_AQUI"
        }
      }
    ]
  }


}
